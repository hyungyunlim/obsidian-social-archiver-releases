{
  "name": "obsidian-social-archiver",
  "version": "2.6.0",
  "description": "Save social media posts directly into your Obsidian vault",
  "main": "main.js",
  "type": "module",
  "author": {
    "name": "Hyungyun Lim"
  },
  "license": "MIT",
  "scripts": {
    "sync:shared": "node scripts/sync-shared.mjs",
    "predev": "npm run sync:shared",
    "dev": "vite build --watch --mode development",
    "build": "npm run build:prod",
    "build:prod": "npm run sync:shared && vite build --mode production",
    "build:source": "npm run sync:shared && vite build --mode source",
    "verify:release": "node scripts/verify-release-metadata.mjs",
    "check:css-source": "node scripts/check-css-source.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx,.svelte",
    "typecheck": "tsc --noEmit",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.4",
    "@types/jsdom": "^27.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "eslint-plugin-obsidianmd": "github:obsidianmd/eslint-plugin",
    "eslint-plugin-svelte": "^2.0.0",
    "jsdom": "^27.0.1",
    "obsidian": "latest",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "tslib": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.12",
    "vitepress": "^1.6.4",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@tiptap/core": "^3.10.1",
    "@tiptap/extension-character-count": "^3.10.1",
    "@tiptap/extension-image": "^3.10.1",
    "@tiptap/extension-link": "^3.10.1",
    "@tiptap/extension-placeholder": "^3.10.1",
    "@tiptap/starter-kit": "^3.10.1",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "leaflet": "^1.9.4",
    "libheif-js": "^1.19.8",
    "p-queue": "^7.4.1",
    "tiptap-markdown": "^0.9.0",
    "zod": "^3.22.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyungyunlim/obsidian-social-archiver-releases"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "social-media",
    "archiver",
    "facebook",
    "linkedin",
    "instagram",
    "tiktok",
    "threads",
    "twitter"
  ]
}
